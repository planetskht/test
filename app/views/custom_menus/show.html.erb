<%- model_class = CustomMenu -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:menu_name) %>:</strong></dt>
  <dd><%= @custom_menu.menu_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sub_menu_name) %>:</strong></dt>
  <dd><%= @custom_menu.sub_menu_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:attachment_type) %>:</strong></dt>
  <dd><%= @custom_menu.attachment_type %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              custom_menus_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_custom_menu_path(@custom_menu), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              custom_menu_path(@custom_menu),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
